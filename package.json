{
  "name": "uniapp-vue3-vite-tailwind-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "uni",
    "deploy": "uni build",
    "dev:app": "uni -p app-plus",
    "build:app": "uni build -p app-plus",
    "dev:wx": "uni -p mp-weixin",
    "build:wx": "uni build -p mp-weixin",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint",
    "husky": "husky install",
    "log": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0 -n changelog.config.ts",
    "lint:pretty": "pretty-quick --staged",
    "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-3081220230817001",
    "@dcloudio/uni-app-plus": "3.0.0-3081220230817001",
    "@dcloudio/uni-components": "3.0.0-3081220230817001",
    "@dcloudio/uni-h5": "3.0.0-3081220230817001",
    "@dcloudio/uni-mp-weixin": "3.0.0-3081220230817001",
    "@dcloudio/uni-ui": "^1.4.28",
    "pinia": "~2.0.36",
    "vue": "^3.2.45",
    "vue-i18n": "^9.1.9",
    "vue-json-pretty": "^2.0.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-automator": "3.0.0-3081220230817001",
    "@dcloudio/uni-cli-shared": "3.0.0-3081220230817001",
    "@dcloudio/uni-helper-json": "^1.0.13",
    "@dcloudio/uni-stacktracey": "3.0.0-3081220230817001",
    "@dcloudio/vite-plugin-uni": "3.0.0-3081220230817001",
    "@types/node": "^17.0.8",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.2",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-custom-config": "^0.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.31",
    "postcss-class-rename-postcss8": "^1.0.2",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "tailwind-one": "^1.0.8",
    "tailwindcss": "^3.3.5",
    "tailwindcss-miniprogram-preset": "^2.1.2",
    "typescript": "^4.9.4",
    "vite": "4.1.4",
    "vite-plugin-style-import": "^2.0.0",
    "vue-types": "^5.1.1"
  },
  "engines": {
    "node": "^18 || >=16"
  }
}
